    "prettier": "^2.8.0",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.1",
    "svg-sprite-loader": "^6.0.11",
    "vite": "^4.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-eslint-parser": "^9.1.0"
